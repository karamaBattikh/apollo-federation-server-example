{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "eslint-config-prettier"],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": 1,
    "max-len": ["error", { "code": 100, "ignoreUrls": true }],
    "no-var": "error",
    "linebreak-style": 0,
    "prettier/prettier": "error",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}